public class TestaBanco {

    public static void main(String[] args) {
        Cliente diego = new Cliente();
        diego.nome = "Diego";
        diego.cpf = "123.423.678-89";
        diego.profissao = "Programador";

        Conta contaDoDiego = new Conta();
        contaDoDiego.deposita(100);

        contaDoDiego.titular = diego;
        System.out.println("CPF: " + contaDoDiego.titular.cpf);
        System.out.println("Titular: " + contaDoDiego.titular.nome);
        System.out.println("Profiss√£o: " + contaDoDiego.titular.profissao);

    }
}
