public class TestaWhile {
    public static void main(String[] args) {
        int contador = 0;

        while(contador <= 10){

            System.out.println(contador);

            contador++;
        }
        System.out.println("***************************************");
        int regressor =  10;

        while (regressor >= 0){

            System.out.println(regressor);

            regressor--;
        }
    }
}
