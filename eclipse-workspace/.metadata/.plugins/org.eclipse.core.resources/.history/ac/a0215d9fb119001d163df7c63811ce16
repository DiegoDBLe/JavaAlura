public class TestaMetodo {
    public static void main(String[] args) {
        Conta contaDiego = new Conta();

        contaDiego.deposita(200);
        System.out.println("Saldo da conta é de R$ " + contaDiego.saldo);

        contaDiego.deposita(50);
        System.out.println("Saldo da conta é de R$ " + contaDiego.saldo);

        contaDiego.saca(20);

        contaDiego.saca(500);

        contaDiego.saca(10);

        Conta contaLevi = new Conta();
        contaLevi.deposita(1000);

        contaLevi.transfere(100, contaDiego);

        contaDiego.transfere(500, contaLevi);
    }
}
